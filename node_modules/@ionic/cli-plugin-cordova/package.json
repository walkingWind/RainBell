  "readme": "# @ionic/cli-plugin-cordova\n\nThis CLI plugin provides Cordova functionality to the CLI. It adds a new CLI\nnamespace (`ionic cordova`). It adds several commands that wrap raw Cordova\ncommands and provide helpful messages and checks, but it also adds a new\ncommand for automatically resizing Cordova icons and splash screens.\n",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/ionic-cli.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run build-js",
    "build-js": "npm run script-prepublish && cp ./resources.json ./dist/resources.json",
    "build-ts": "tsc",
    "clean": "rm -rf ./dist",
    "lint": "tslint -c ../../tslint.json \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "script-prepublish": "cli-scripts-prepublish cli-plugin-cordova",
    "test": "jest",
    "watch": "concurrently -p \"{name} \" -n \"TS,JS\" -c \"cyan.bold,yellow.dim\" \"npm run watch-ts\" \"npm run watch-js\"",
    "watch-js": "cross-env-shell IONIC_PREPUBLISH_PLUGIN_VERSION=${npm_package_version}-local \"npm run watch-js-chokidar\"",
    "watch-js-chokidar": "chokidar --silent \"dist/**/*.js\" -c \"npm run build-js\"",
    "watch-ts": "tsc -w"
  },
  "types": "./dist/main.d.ts",
  "version": "1.4.1"
